<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>hu.icellmobilsoft.sampler.testsuite</groupId>
        <artifactId>ts-sample</artifactId>
        <version>2.0.0-SNAPSHOT</version>
    </parent>
    <artifactId>ts-sample-grpc-server</artifactId>
    <dependencies>
        <dependency>
            <groupId>hu.icellmobilsoft.sampler.testsuite</groupId>
            <artifactId>ts-common-rest</artifactId>
            <exclusions>
                <exclusion>
                    <groupId>hu.icellmobilsoft.coffee</groupId>
                    <artifactId>coffee-dto-gen</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
    </dependencies>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-failsafe-plugin</artifactId>
            </plugin>
        </plugins>
        <resources>
            <resource>
                <directory>src/test/resources</directory>
                <filtering>true</filtering>
            </resource>
            <resource>
                <directory>../../../etc/config/testsuite</directory>
                <includes>
                    <include>META-INF/roaster-*.yml</include>
                </includes>
                <filtering>true</filtering>
            </resource>
        </resources>
    </build>
</project>